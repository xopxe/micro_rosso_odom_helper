; https://docs.platformio.org/page/projectconf.html

[env:pico32]
platform = espressif32
board = pico32
framework = arduino
board_microros_distro = humble
board_microros_transport = serial

lib_deps = 
    https://github.com/xopxe/micro_rosso_platformio.git
